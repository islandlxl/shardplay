<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.islandlxl.biz.mapper.DemoShowMapper">
    <select id="getSystemList" resultType="java.util.Map">
        select * from `sys_user`
    </select>

    <update id="updateSystemList">
        update `sys_user` set  username='111'  where id=1;
    </update>

    <select id="getStudentList" resultType="java.util.Map">
        select * from `edu_student`
    </select>

    <select id="getFinanceList" resultType="java.util.Map">
        select * from `fin_major_tuition_grade`
    </select>

    <select id="getBizList" resultType="java.util.Map">
        select * from `biz_trade`
    </select>


</mapper>